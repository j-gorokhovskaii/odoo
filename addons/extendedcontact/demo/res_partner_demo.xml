<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Demo data for extended contact fields -->
        
        <!-- John Smith - Complete profile example -->
        <record id="demo_partner_john_smith" model="res.partner">
            <field name="name">John Smith</field>
            <field name="email">john.smith@example.com</field>
            <field name="is_company" eval="False"/>
            <field name="birthdate" eval="(DateTime.today() - relativedelta(years=35, months=2, days=15)).strftime('%Y-%m-%d')"/>
            <field name="gender">male</field>
            <field name="marital_status">married</field>
            <field name="height" eval="180.5"/>
            <field name="weight" eval="85.2"/>
            <field name="build">athletic</field>
            <field name="complexion">medium</field>
            <field name="eye_color">blue</field>
            <field name="hair_color">brown</field>
            <field name="hair_style">short</field>
            <field name="blood_type">o_positive</field>
            <field name="distinguishing_features">Small scar on left eyebrow</field>
            <field name="ethnicity">not_hispanic</field>
            <field name="race">white</field>
            <field name="primary_language">English</field>
            <field name="country_of_birth" ref="base.us"/>
            <field name="citizenship_status">citizen</field>
            <field name="ssn">123-45-6789</field>
            <field name="driver_license_number">DL123456789</field>
            <field name="driver_license_state" ref="base.state_us_ca"/>
            <field name="occupation">Software Engineer</field>
            <field name="employer">Tech Solutions Inc.</field>
            <field name="education_level">bachelors</field>
            <field name="military_service" eval="False"/>
            <field name="medical_conditions">Mild seasonal allergies</field>
            <field name="emergency_contact_name">Jane Smith</field>
            <field name="emergency_contact_phone">+1-555-0102</field>
            <field name="emergency_contact_relationship">Spouse</field>
        </record>

